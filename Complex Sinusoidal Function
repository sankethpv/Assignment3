f=[1 3 5 10 20];
for i=1:5
    %frequency=f
    t=0:1:2*pi;
    %u=frequency*time domain
    u=f(i)*t;
    %s=sine waves
    s=sin(2*pi*f(i));
    %r=rel part of the sinusoidal waves
    r=s*cos(u);
    %i=imaginary part of the sinusoidal wave
    w=s*sin(u);
    %complex sinusoidal wave=real part+ imaginary part
    y=w+r;
    k=0;
    k=k+y;
end
 figure
    plot(k)
